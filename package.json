{
  "name": "easyjre",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://justinmahar.github.io/easyjre/",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "copy-cname": "mkdir -p build && cp CNAME ./build",
    "predeploy": "rm -rf build && npm run build",
    "deploy": "gh-pages -d build",
    "updoc": "git add README.md && git diff-index --quiet HEAD README.md || git commit README.md -m \"Update README.\" && git push",
    "updog": "npm run updoc"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@material-ui/core": "^4.0.0",
    "@material-ui/icons": "^3.0.2",
    "@types/jest": "24.0.13",
    "@types/node": "12.0.2",
    "@types/react": "16.8.18",
    "@types/react-dom": "16.8.4",
    "@types/react-router-dom": "^4.3.3",
    "notistack": "^0.8.4",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router-dom": "^5.0.0",
    "react-scripts": "5.0.1",
    "typeface-roboto": "0.0.54",
    "typescript": "3.4.5"
  },
  "devDependencies": {
    "gh-pages": "^2.0.1"
  }
}
